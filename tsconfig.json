{
    "compilerOptions": {
        "target": "ES6",
        "jsx": "react",
        "noEmitOnError": true,
        "noUnusedLocals": true,
        "removeComments": true,
        "noImplicitAny": true,
        "esModuleInterop": true,
        "moduleResolution": "node",
        "baseUrl": "./",
        "outDir": "./dist/",
        "rootDir": "src",
    },
    "exclude": [
        "node_modules"
    ]
}